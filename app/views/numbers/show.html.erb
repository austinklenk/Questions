<div class="panel panel-default">
	<div class="panel-heading">
		<h4>Number for <%= @number.created_at.strftime("%B %e, %Y, %I:%M %P") %></h4>
	</div>
	<div class="panel-body">
		<p>
		<strong>Time &amp; Date</strong>
		<%= @number.created_at.strftime("%B %e, %Y, %I:%M %P") %>
		</p>
		<p>
		  <strong>Name:</strong>
		  <%= @number.name %>
		</p>

		<p>
		  <strong>Insulin Dose:</strong>
		  <%= @number.insulin_dose%>
		</p>

		<p>
		  <strong>Description:</strong>
		  <%= @number.description %>
		</p>
	</div>
	<div class="panel-footer">
		<%= link_to 'Edit', edit_number_path(@number), class: 'btn btn-default' %> |
		<%= link_to 'Back', numbers_path, class: 'btn btn-default' %>
	</div>
</div>