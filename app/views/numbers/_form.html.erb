<%= bootstrap_form_for(@number) do |f| %>
  <%= f.alert_message "Please fix the errors below." %>
  <%= f.text_field :name, label: "Blood Sugar Number" %>
  <%= f.number_field :insulin_dose, label: "Insulin Dose" %>
  <%= f.text_area :description, label: "Activity or Description" %>
  <%= f.submit "Save Number", class: 'btn btn-success' %> | 
  <%= link_to 'Back', numbers_path, class: 'btn btn-default' %>
<% end %>